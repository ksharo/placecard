{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": [1],
        "no-empty": ["error"],
        "quotes": ["error", "double", {"allowTemplateLiterals": true}],
        "eqeqeq": ["error"],
        "semi-style": ["error"],
        "eol-last": ["error", "always"]
    }
}
